{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "pitData": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "scouterInitials": { "type": "string" },
          "teamNumber": { "type": "string" },
          "drivetrainType": { "type": "string" },
          "weight": { "type": "string" },
          "climbingAbility": { "type": "string" },
          "maxHopperSize": { "type": "string" },
          "trenchDriveAbility": { "type": "boolean" },
          "rotatableShooter": { "type": "boolean" },
          "intakeFromDepot": { "type": "boolean" },
          "intakeFromOutpost": { "type": "boolean" },
          "photoTaken": { "type": "boolean" },
          "imageSrc": { "type": "string" }
        },
        "additionalProperties": false
      }
    },
    "matchData": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "matchNumber": { "type": "string" },
          "alliance": { "type": "string" },
          "scouterInitials": { "type": "string" },
          "selectTeam": { "type": "string" },
          "autoRobotPositions": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "x": { "type": "number" },
                "y": { "type": "number" },
                "driveType": { "type": "string" },
                "shotInfo": {
                  "type": "object",
                  "properties": {
                    "fuelShot": { "type": "number" }
                  },
                  "additionalProperties": false
                }
              },
              "additionalProperties": false
            }
          },
          "fuelShotAndSourceInfo": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "source": { "type": "string" },
                "fuelShot": { "type": "number" }
              },
              "additionalProperties": false
            }
          },
          "comments": { "type": "string" },
          "broken": { "type": "boolean" },
          "brokenDownTime": { "type": "string" },
          "playedDefense": { "type": "boolean" },
          "defenseTime": { "type": "string" },
          "defenseSkill": { "type": "string" },
          "playedDefenseOn": { "type": "string" }
        },
        "additionalProperties": false
      }
    }
  },
  "required": ["pitData", "matchData"],
  "additionalProperties": false
}
